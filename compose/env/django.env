DJANGO_SECRET_KEY=devsecret
DJANGO_DEBUG=1
POSTGRES_DB=valhalla
POSTGRES_USER=valhalla
POSTGRES_PASSWORD=valhalla
DJANGO_SUPERUSER_USERNAME=admin
DJANGO_SUPERUSER_EMAIL=admin@example.com
DJANGO_SUPERUSER_PASSWORD=admin123
DJANGO_STAFF_USERNAME=staff
DJANGO_STAFF_EMAIL=staff@example.com
DJANGO_STAFF_PASSWORD=changeme

# Celery worker concurrency
CELERY_CONCURRENCY=1

# Ports and project name (used by docker-compose variable substitution)
# Use explicit project name so services started from inside containers use the correct project
PROJECT_NAME=valhalla-admin
COMPOSE_PROJECT_NAME=valhalla-admin
DJANGO_PORT=8082
REDIS_PORT=6379

# GTFS source API endpoint (default: transport.data.gouv)
GTFS_SOURCE_API_URL=https://transport.data.gouv.fr/api/datasets?format=gtfs

# System-wide timezone (e.g., Europe/Paris, UTC)
SYSTEM_TIMEZONE=Europe/Paris

SERVER_NAME=localhost

# Conteneur cible pour l'ex√©cution des builds Valhalla (ex: valhalla-admin-valhalla)
VALHALLA_BUILD_CONTAINER=valhalla-admin-valhalla